<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="lp.reminiscens.crawler.entities.Time_Interval" table="reminiscens.Time_Interval">
    <id column="time_interval_id" name="time_interval_id" type="int">
      <generator class="native"/>
    </id>
    <property name="start_date">
      <column name="startdate"/>
    </property>
    <property name="end_date">
      <column name="enddate"/>
    </property>
    <property name="duration_unit">
      <column name="duration_unit"/>
    </property>
    <property name="duration_amount">
      <column name="duration_amount"/>
    </property>
    <property name="is_fuzzy">
      <column name="is_fuzzy"/>
    </property>
    <many-to-one cascade="all" column="fuzzy_startdate" name="startdate" unique="true"/>
    <many-to-one cascade="all" column="fuzzy_enddate" name="enddate" unique="true"/>
  </class>
</hibernate-mapping>
